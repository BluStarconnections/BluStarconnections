<?xml version="1.0"?>
<odoo>

    <record id="crm_lead_form_inherit" model="ir.ui.view">
        <field name="name">crm.lead.form.inherit</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//group" position="after">
                <group col="4">
                    <field name="partner_name"/>
                    <field name="contact_first_name"/>
                    <field name="contact_last_name"/>
                    <field name="phonetic_spelling"/>
                    <field name="function"/>
                    <field name="contact_phone" widget="phone"/>
                    <field name="primary_phone_type"/>
                    <field name="primary_email" widget="email"/>
                    <field name="secondary_first_name"/>
                    <field name="secondary_last_name"/>
                    <field name="sec_title"/>
                    <field name="secondary_contact_phone" widget="phone"/>
                    <field name="secondary_phone_type"/>
                    <field name="secondary_email" widget="email"/>
                    <field name="appt_status" widget="selection"/>
                    <field name="next_appointment"/>
                    <field name="appointment_type" widget="selection"/>
                    <field name="ein"/>
                    <field name="ein_date"/>
                    <field name="assets" widget="monetary"/>
                    <field name="number_of_participants"/>
                    <field name="fund_manager"/>
                    <field name="tpa"/>
                    <field name="has_advisor"/>
                    <field name="area_of_concern"/>
                    <field name="surveyor"/>
                    <field name="result_code"/>
                    <field name="financial_advisor"/>
                </group>
            </xpath>
