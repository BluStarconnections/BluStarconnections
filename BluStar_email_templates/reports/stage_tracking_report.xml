<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="template_report_stage_tracing">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <t t-foreach="docs" t-as="doc">
                    <div class="page">
                        <t t-set="data_dict" t-value="doc.get_report_vals()"/>
                        <t t-foreach="data_dict" t-as="item">
                            <h3>
                                <t t-esc="item"/>
                            </h3>
                            <table class="table" style="page-break-after:always">
                                <thead>
                                    <th>Lead title</th>
                                    <th>Previous stage</th>
                                    <th>New stage</th>
                                </thead>
                                <tbody>
                                    <t t-foreach="data_dict[item]" t-as="data">
                                        <tr>
                                            <td t-esc="data.get('lead')"/>
                                            <td t-esc="data.get('prev_stage')"/>
                                            <td t-esc="data.get('new_stage')"/>
                                        </tr>
                                    </t>
                                </tbody>
                            </table>
                        </t>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>